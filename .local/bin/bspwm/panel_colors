#!/bin/sh

eval "$(xres2var)"

COLOR_DEFAULT_FG="$foreground"
COLOR_DEFAULT_BG="$background"
COLOR_MONITOR_FG="$foreground"
COLOR_MONITOR_BG="$background"
COLOR_FOCUSED_MONITOR_FG="$color15"
COLOR_FOCUSED_MONITOR_BG="$color4"
COLOR_FREE_FG="$color0"
COLOR_FREE_BG="$background"
COLOR_FOCUSED_FREE_FG="$color4"
COLOR_FOCUSED_FREE_BG="$background"
COLOR_OCCUPIED_FG="$color3"
COLOR_OCCUPIED_BG="$background"
COLOR_FOCUSED_OCCUPIED_FG="$background"
COLOR_FOCUSED_OCCUPIED_BG="$color4"
COLOR_URGENT_FG="$foreground"
COLOR_URGENT_BG="$color1"
COLOR_FOCUSED_URGENT_FG="$color1"
COLOR_FOCUSED_URGENT_BG="$foreground"
COLOR_STATE_FG="$color10"
COLOR_STATE_BG="$background"
COLOR_TITLE_FG="$color15"
COLOR_TITLE_BG="$background"
COLOR_SYS_FG="$foreground"
COLOR_SYS_BG="$background"


