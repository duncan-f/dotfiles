# mode for launching various applications
set $apps_menu " (d)  (b)  (s) Sublime Text (c) Visual Studio Code "
mode $apps_menu {
    # start File Manager
    bindsym d exec $term -e ranger, mode "default"

    # start Web Browser
    bindsym b exec --no-startup-id firefox, mode "default"
    bindsym w exec --no-startup-id $BROWSER, mode "default"

   	# start Mail Reader
    bindsym m exec $term -e neomutt, mode "default"

    # start Sublime Text
    bindsym s exec --no-startup-id subl3, mode "default"

    # start Visual Studio Code
   	bindsym c exec --no-startup-id code, mode "default"

    # back to normal: Enter or Escape
    bindsym Return mode "default"
    bindsym Escape mode "default"
}
